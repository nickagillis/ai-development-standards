# GitHub API Configuration
# Get your token from: https://github.com/settings/tokens
GITHUB_TOKEN=your_github_token_here

# arXiv API Configuration (no auth required)
ARXIV_ENABLED=true
ARXIV_MAX_RESULTS=50

# AI Intelligence System Configuration
AI_INTELLIGENCE_ENABLED=true
AI_INTELLIGENCE_CHECK_INTERVAL=1800000  # 30 minutes
AI_INTELLIGENCE_AUTO_UPDATE_DOCS=false  # Set to true for automatic document updates

# Monitoring Configuration
MONITOR_AGENTS=true
MONITOR_MEMORY=true
MONITOR_INFRASTRUCTURE=true
MONITOR_EMERGING=true

# Rate Limiting
GITHUB_RATE_LIMIT=5000  # Requests per hour with token
ARXIV_RATE_LIMIT=100   # Requests per hour (be respectful)

# Logging
LOG_LEVEL=info
LOG_FILE=ai-intelligence.log

# Backup and Safety
BACKUP_ENABLED=true
BACKUP_DIRECTORY=.backup

# Production Settings
NODE_ENV=development  # Set to 'production' for auto-commits